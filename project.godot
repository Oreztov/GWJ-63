; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Gwj-63"
run/main_scene="res://scenes/game_main.tscn"
config/features=PackedStringArray("4.1", "GL Compatibility")
config/icon="res://textures/icon.svg"

[autoload]

PowerManager="*res://scripts/PowerManager.gd"
SpawnManager="*res://scripts/SpawnManager.gd"
MouseManager="*res://scenes/mouse_manager.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=3
mouse_cursor/custom_image="res://textures/cursors/cursor_new.png"
mouse_cursor/custom_image_hotspot=Vector2(10, 5)

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
